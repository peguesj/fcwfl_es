<%= simple_form_for(@result) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :rating %>
    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.input :zip %>
    <%= f.input :tel %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
