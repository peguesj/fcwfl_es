<p id="notice"><%= notice %></p>

<h1>Listing Yelpfinds</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @yelpfinds.each do |yelpfind| %>
      <tr>
        <td><%= link_to 'Show', yelpfind %></td>
        <td><%= link_to 'Edit', edit_yelpfind_path(yelpfind) %></td>
        <td><%= link_to 'Destroy', yelpfind, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Yelpfind', new_yelpfind_path %>
